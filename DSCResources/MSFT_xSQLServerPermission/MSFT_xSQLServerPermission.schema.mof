[ClassVersion("1.0.0.0"), FriendlyName("xSQLServerPermission")]
class MSFT_xSQLServerPermission : OMI_BaseResource
{
    [Key, Description("TThe name of the SQL instance to be configured.")] String InstanceName;
    [Required, Description("The host name of the SQL Server to be configured. Default value is $env:COMPUTERNAME.")] String NodeName;
    [Write, Description("If the permission should be present or absent. Default value is 'Present'."), ValueMap{"Present","Absent"}, Values{"Present","Absent"}] String Ensure;
    [Required, Description("The login to which permission will be set.")] String Principal;
    [Write, Description("The permission to set for the login. Valid values are AlterAnyAvailabilityGroup, ViewServerState or AlterAnyEndPoint."), ValueMap{"AlterAnyAvailabilityGroup","ViewServerState","AlterAnyEndPoint"}, Values{"AlterAnyAvailabilityGroup","ViewServerState","AlterAnyEndPoint"}] String Permission[];
};
